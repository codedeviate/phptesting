{
    "version": "1.0",
    "description": "PHP testing environments from PHP 7.4 up to PHP 8.2",
    "scripts": {
        "start": "docker-compose down && docker-compose up -d",
        "stop": "docker-compose down",
        "restart": "docker-compose restart",
        "logs": "docker-compose logs -f",
        "build": "docker-compose build",
        "enter:php56": "docker exec -it phptesting-php56-1 ash",
        "enter:php70": "docker exec -it phptesting-php70-1 ash",
        "enter:php71": "docker exec -it phptesting-php71-1 ash",
        "enter:php72": "docker exec -it phptesting-php72-1 ash",
        "enter:php73": "docker exec -it phptesting-php73-1 ash",
        "enter:php74": "docker exec -it phptesting-php74-1 ash",
        "enter:php80": "docker exec -it phptesting-php80-1 ash",
        "enter:php81": "docker exec -it phptesting-php81-1 ash",
        "enter:php82": "docker exec -it phptesting-php82-1 ash"
    }
}